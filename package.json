{
  "name": "recurr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf build/ && babel ./ --out-dir build/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "nodemon --ignore ./public --exec babel-node ./bin/www",
    "dev": "nodemon --ignore ./public --exec babel-node ./bin/www",
    "css-build": "node-sass --omit-source-map-url sass/style.scss public/stylesheets/style.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "bulma": "^0.9.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "mariadb": "^2.5.2",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "node-sass": "^5.0.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "sequelize": "^6.3.5",
    "tunnel-ssh": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2"
  }
}
